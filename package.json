{
  "name": "@bigmistqke/view.gl",
  "version": "0.1.3",
  "description": "ðŸªŸ Utilities for managing WebGL resources.",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./tag": {
      "import": "./dist/tag.mjs",
      "types": "./dist/tag.d.mts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev:build": "vite build dev",
    "dev:preview": "vite preview dev",
    "dev:watch": "concurrently \"pnpm dev:preview\" \"vite dev:build --watch\"",
    "dev": "vite serve dev",
    "format": "prettier -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "prepublish": "pnpm build",
    "test": "vitest --run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "WebGL"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.5.1",
  "devDependencies": {
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.1",
    "happy-dom": "^20.0.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
